<h1>New document</h1>

<% form_for(:document, :url => documents_path, :html => { :multipart => true }) do |f| %>
	<%= f.error_messages %>

	<div id="field" class="round">
		<label for="document">Upload An Example:</label>
		<%= f.file_field :uploaded_data %>
	</div>
	<div id="action" class="round">
		<%= submit_tag 'Create' %>
	</div>
<% end %>


<%= link_to 'Back', documents_path %>
